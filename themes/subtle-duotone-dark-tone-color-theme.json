  "tokenColors": [
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a4befe"
      }
    },
    {
      "name": "Entity name function",
      "scope": [
        "entity.name.function",
        "entity.name.type.class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a4befe"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6782c5",
        "fontStyle": ""
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffa8c1"
      }
    },
    {
      "name": "Keyword other",
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffa8c1"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "delimiter",
        "bracket",
        "brace",
        "paren",
        "delimiter.tag",
        "punctuation.tag",
        "tag.html",
        "tag.xml",
        "meta.property-value punctuation.separator.key-value",
        "punctuation.definition.metadata.md",
        "string.link.md",
        "meta.brace",
        "keyword.operator.relational"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#6782c5"
      }
    },
    {
      "name": "Punctuation accessor",
      "scope": "punctuation.accessor",
      "settings": {
        "foreground": "#2bf0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.end.js",
        "punctuation.section.embedded.begin.metatag.php",
        "punctuation.section.embedded.end.metatag.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Object property",
      "scope": "variable.other.object.property",
      "settings": {
        "foreground": "#ebf0ff",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.property-value.string",
        "support.constant.property-value.string",
        "meta.structure.dictionary.value.json string.quoted.double.json",
        "meta.structure.dictionary.json string.quoted.double.json",
        "meta.preprocessor string",
        "support.function.variable.quoted.single"
      ],
      "settings": {
        "foreground": "#ffbb99"
      }
    },
    {
      "name": "Primitive Literals",
      "scope": [
        "constant.numeric",
        "meta.property-value.numeric",
        "support.constant.property-value.numeric",
        "meta.property-value.color",
        "support.constant.property-value.color",
        "constant.language"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "User names",
      "scope": [
        "constant.character",
        "constant.other",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "entity.name",
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "support.type.property-name",
        "entity.name.tag.table",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "HTML classes",
      "scope": [
        "entity.other.attribute-name.html",
      ],
      "settings": {
        "foreground": "#a4befe",
        "fontStyle": ""
      }
    },
    {
      "name": "Embedded Elixir",
      "scope": [
        "source.elixir.embedded",
      ],
      "settings": {
        "foreground": "#ff5283",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "meta.property-value.keyword",
        "support.constant.property-value.keyword",
        "meta.preprocessor.keyword",
        "keyword.other.use",
        "keyword.other.function.use",
        "keyword.other.namespace",
        "keyword.other.new",
        "keyword.other.special-method",
        "keyword.other.unit",
        "keyword.other.use-as"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#f4769a"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.type.ts",
        "storage.type.var.ts",
        "storage.type.js",
        "storage.type.var.js",
        "storage.type.const.ts",
        "storage.type.let.ts",
        "storage.type.let.js",
        "storage.type.const.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#f4769a"
      }
    },
    {
      "name": "Storage modifier",
      "scope": [
        "storage.modifier",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffa8c1"
      }
    },
    {
      "name": "Keyword Type",
      "scope": [
        "keyword.type",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#fa769a"
      }
    },
    {
      "name": "Storage type annotation",
      "scope": [
        "storage.type.annotation"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#44506f"
      }
    },
    {
      "name": "Pointer, access, etc",
      "scope": [
        "meta.ptr",
        "meta.pointer",
        "meta.address",
        "meta.array.cxx"
      ],
      "settings": {}
    },
    {
      "name": "Preprocessor",
      "scope": "meta.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Library",
      "scope": [
        "support.type",
        "support.class",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#f4769a"
      }
    },
    {
      "name": "Library",
      "scope": [
        "support.function",
        "support.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f14c4c"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#f14c4c"
      }
    },
    // Markdown
    {
      "name": "Markdown Titles",
      "scope": [
        "entity.name.section.markdown",
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff5283"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown",
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Default Text",
      "scope": [
        "text.html.markdown",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "name": "Markdown Title Hashes",
      "scope": [
        "punctuation.definition.heading.markdown",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#6782c5"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a4befe"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    // Makefile
    {
      "name": "Makefile Variables",
      "scope": [
        "variable.language.makefile",
        "variable.other.makefile",
        "variable.other.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf0ff"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
