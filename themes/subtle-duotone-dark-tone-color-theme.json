    // Salesforce specific
    {
      "name": "Sharing",
      "scope": "sharing",
      "settings": {
        "foreground": "#ffa8c1",
        "fontStyle": ""
      }
    },
    {
      "name": "Queries",
      "scope": "keyword.operator.query",
      "settings": {
        "foreground": "#ffa8c1",
        "fontStyle": "bold"
      }
