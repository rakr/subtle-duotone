  "tokenColors": [
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#886724"
      }
    },
    {
      "name": "Entity name function",
      "scope": [
        "entity.name.function",
        "entity.name.type.class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2d2006"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2d2006"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B6AD9A"
      }
    },
    {
      "name": "Operator and Control",
      "scope": [
        "keyword.operator",
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#063289"
        // "foreground": "#2076c9"
        // "foreground": "#2d2006"
      }
    },
    {
      "name": "Keyword other",
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2076c9"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "delimiter",
        "bracket",
        "brace",
        "paren",
        "delimiter.tag",
        "punctuation.tag",
        "tag.html",
        "tag.xml",
        "meta.property-value punctuation.separator.key-value",
        "punctuation.definition.metadata.md",
        "string.link.md",
        "meta.brace",
        "keyword.operator.relational"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#B6AD9A"
      }
    },
    {
      "name": "Punctuation accessor",
      "scope": "punctuation.accessor",
      "settings": {
        "foreground": "#3D2C08",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.end.js",
        "punctuation.section.embedded.begin.metatag.php",
        "punctuation.section.embedded.end.metatag.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Object property",
      "scope": "variable.other.object.property",
      "settings": {
        "foreground": "#2D2006",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.property-value.string",
        "support.constant.property-value.string",
        "meta.structure.dictionary.value.json string.quoted.double.json",
        "meta.structure.dictionary.json string.quoted.double.json",
        "meta.preprocessor string",
        "support.function.variable.quoted.single"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2c5282"
      }
    },
    {
      "name": "Primitive Literals",
      "scope": [
        "constant.numeric",
        "meta.property-value.numeric",
        "support.constant.property-value.numeric",
        "meta.property-value.color",
        "support.constant.property-value.color",
        "constant.language"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "User names",
      "scope": [
        "constant.character",
        "constant.other",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "entity.name",
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "support.type.property-name",
        "entity.name.tag.table",
        "entity.name.tag",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2d2006"
      }
    },
    {
      "name": "HTML classes",
      "scope": [
        "entity.other.attribute-name.html",
      ],
      "settings": {
        "foreground": "#886724",
        "fontStyle": ""
      }
    },
    {
      "name": "Embedded Elixir",
      "scope": [
        "source.elixir.embedded",
      ],
      "settings": {
        "foreground": "#477EEB",
        "fontStyle": ""
      }
    },
    {
      "name": "Embedded Elixir Tags",
      "scope": [
        "punctuation.section.embedded.elixir",
      ],
      "settings": {
        "foreground": "#DD4A68",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "meta.property-value.keyword",
        "support.constant.property-value.keyword",
        "meta.preprocessor.keyword",
        "keyword.other.use",
        "keyword.other.function.use",
        "keyword.other.namespace",
        "keyword.other.new",
        "keyword.other.special-method",
        "keyword.other.unit",
        "keyword.other.use-as"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#063289"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.type.ts",
        "storage.type.var.ts",
        "storage.type.js",
        "storage.type.var.js",
        "storage.type.const.ts",
        "storage.type.let.ts",
        "storage.type.let.js",
        "storage.type.const.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#063289"
      }
    },
    {
      "name": "Storage modifier",
      "scope": [
        "storage.modifier",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2076c9"
      }
    },
    {
      "name": "Keyword Type",
      "scope": [
        "keyword.type",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#708090"
      }
    },
    {
      "name": "Storage type annotation",
      "scope": [
        "storage.type.annotation"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#AEB1B7"
      }
    },
    {
      "name": "Pointer, access, etc",
      "scope": [
        "meta.ptr",
        "meta.pointer",
        "meta.address",
        "meta.array.cxx"
      ],
      "settings": {}
    },
    {
      "name": "Preprocessor",
      "scope": "meta.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Library",
      "scope": [
        "support.type",
        "support.class",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#063289"
      }
    },
    {
      "name": "Library",
      "scope": [
        "support.function",
        "support.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f14c4c"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#f14c4c"
      }
    },
    {
      "name": "Markdown Title Hash",
      "scope": [
        "punctuation.definition.heading.md",
        "entity.name.type.md",
        "beginning.punctuation"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Markdown titles",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#063289"
      }
    },
    {
      "name": "Markdown paragraphs",
      "scope": [
        "meta.paragraph"
      ],
      "settings": {
        "foreground": "#2d2006",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown quotes",
      "scope": [
        "markup.quote meta.paragraph"
      ],
      "settings": {
        "foreground": "#718ecd",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown Raw",
      "scope": [
        "markup.raw",
        "markup.inline.raw",
        "markup.fenced",
        "markup.fenced_code"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#727981"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.link",
        "string.other.link.title",
        "string.other.link.description",
        "meta.link.inline",
        "meta.image.inline"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Makefile Variables",
      "scope": [
        "variable.language.makefile",
        "variable.other.makefile",
        "variable.other.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "CSS Tag name",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "meta.property-name.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2D2006"
      }
    },
    {
      "name": "Local variables",
      "scope": [
        "entity.name.variable.local"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2d2006"
      }
    },
    // Salesforce specific
    {
      "name": "Sharing",
      "scope": "sharing",
      "settings": {
        "foreground": "#2076c9",
        "fontStyle": ""
      }
    },
    {
      "name": "Queries",
      "scope": "keyword.operator.query",
      "settings": {
        "foreground": "#2076c9",
        "fontStyle": "bold"
      }
    }
  ]
}
